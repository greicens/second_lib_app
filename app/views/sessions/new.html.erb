<%= form_for @user, url: "/sessions", method: "post" do |f| %>
<div class="form-group">
  <%= f.email_field :email, placeholder: "email" %>
</div>
<div class="form-group">
  <%= f.password_field :password_field, placeholder: "password" %>
</div>
<div class="form-group">
  <%= f.submit "Login" %>
  <% end %>
</div>
