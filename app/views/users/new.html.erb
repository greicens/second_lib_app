<div class="form-group">
  <%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.text_field :first_name, placeholder: "first_name" %>
  </div>
  <div class="form-group">
    <%= f.text_field :last_name, placeholder: "last_name" %>
  </div>
  <div class="form-group">
    <%= f.email_field :email, placeholder: "email" %>
  </div>
  <div class="form-group">
    <%= f.password_field :password, placeholder: "password" %>
  </div>
  <div class="form-group">
    <%= f.submit "Sign Up" %>
    <% end %>
  </div>

</div>
